  <url value="https://profiles.ihe.net/QRPH/CCG/StructureDefinition/ccg-signature"/>
  <version value="1.0.0"/>
  <name value="CCG_Signature"/>
  <title value="CCG CARD Digital Signature"/>
  <status value="active"/>
  <date value="2025-10-02T10:26:47-05:00"/>
  <publisher value="IHE QRPH Technical Committee"/>
  <contact>
    <telecom>
      <system value="url"/>
      <value value="https://www.ihe.net/ihe_domains/quality_research_and_public_health/"/>
    </telecom>
