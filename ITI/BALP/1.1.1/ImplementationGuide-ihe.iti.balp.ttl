       ];
       fhir:ImplementationGuide.definition.resource.reference [
         fhir:link <https://profiles.ihe.net/ITI/BALP/StructureDefinition/IHE.BasicAudit.PrivacyDisclosure.Recipient>;
         fhir:Reference.reference [ fhir:value "StructureDefinition/IHE.BasicAudit.PrivacyDisclosure.Recipient" ]
       ];
       fhir:ImplementationGuide.definition.resource.name [ fhir:value "Audit Event for a Privacy Disclosure as recorded by a Recipient" ];
       fhir:ImplementationGuide.definition.resource.description [ fhir:value "Defines constraints on the AuditEvent Resource to record when a Privacy Disclosure is detected at the Recipient of the data.\n\n- Import event\n- shall have source of itself\n- shall have a source agent\n- shall have a recipient agent\n- may have user, app, organization agent(s)\n  - combine with the Security Token pattern\n- may, if known, have the custodian that released the data\n- may, if known, have the authorizer that represented the patient (may be the patient)\n- shall have a patient entity\n- shall have a set identity entity" ];
       fhir:ImplementationGuide.definition.resource.exampleBoolean [ fhir:value "false"^^xsd:boolean ]
     ], [
